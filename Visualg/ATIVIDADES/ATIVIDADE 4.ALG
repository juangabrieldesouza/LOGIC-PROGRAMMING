algoritmo "ATIVIDADE 4"
var
 N1, N2, S: Inteiro
 
inicio
     Escreval("Somas de dois numeros inteiros...")
     Escreva("Digite o primeiro número: ")
     Leia(N1)
     Escreva("Digite o segundo número: ")
     Leia(N2)
     S <- N1 + N2
     Escreval("A soma dos números,", N1, " com o número ", N2, " é igual a ", S)
fimalgoritmo